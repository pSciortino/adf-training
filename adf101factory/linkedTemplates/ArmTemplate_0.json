				"folder": {
					"name": "demo_Attendees"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": "Patrick Sciortino.txt",
						"folderPath": "Attendees",
						"container": "adf-traning"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
