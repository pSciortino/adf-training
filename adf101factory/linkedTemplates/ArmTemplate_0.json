				"typeProperties": {
					"host": "[parameters('psciortinoLinkedService_properties_typeProperties_host')]",
					"userId": "[parameters('psciortinoLinkedService_properties_typeProperties_userId')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('psciortinoLinkedService_password')]"
